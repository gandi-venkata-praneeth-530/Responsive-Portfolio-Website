{
  "extends": [
    "development"
  ],
  "hints": {
    "disown-opener": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-wrap",
          "text-wrap: nowrap",
          "animation-range",
          "animation-timeline"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 139",
    "not chrome <= 109",
    "not firefox <= 139",
    "not ios_saf <= 18.5",
    "not safari <= 18.5"
  ]
}